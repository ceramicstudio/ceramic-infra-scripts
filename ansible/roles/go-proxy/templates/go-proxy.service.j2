[Unit]
Description=go-proxy
After=network-online.target

[Service]
Type=simple
User=ceramic
LimitNOFILE=65535
ExecStart=/usr/local/bin/go-proxy
EnvironmentFile={{ data_store_mount_path }}/go-proxy.env

Restart=always
RestartSec=1
StartLimitInterval=0
NoNewPrivileges=yes
[Install]
WantedBy=multi-user.target
